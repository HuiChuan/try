<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jd.dao.HelloMapper">
    <insert id="insertHello" parameterType="Hello">
        insert into hello (name ,msg) values(#{name}, #{msg})
    </insert>
</mapper>